<div class="contents row">
  <%= form_tag("/tweets/#{@twee.id}",method: :patch ) do %>
    <h3>
      編集する
    </h3>
    <input type="text" name="image" value="<%= @twee.image %>" placeholder="Image Url" autofocus="true">
    <textarea name="text" placeholder="text" rows="10" cols="30"><%= @twee.text %></textarea>
    <input type="submit" value="SENT">
  <% end %>
</div>